{
  "name": "b2lunch",
  "version": "0.1.0",
  "description": "B2 Lunch App",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": "https://bitbucket.org/b2cloud/telstra-homesite-web",
  "scripts": {
    "start": "node server.js",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src test",
    "commit": "git-cz",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./test/setup.js --require ignore-styles",
    "test:watch": "npm test -- --watch",
    "check": "npm run lint && npm run test && flow check ",
    "build": "cross-env --env=dist webpack src/index.js --config cfg/dist.js",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "dist": "npm run copy & webpack --env=dist",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist"
  },
  "author": {
    "name": "Matt Yao",
    "email": "mattyao@b2cloud.com.au",
    "url": "localhost"
  },
  "devEngines": {
    "node": ">=5"
  },
  "keywords": [
    "redux",
    "redux-app"
  ],
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
